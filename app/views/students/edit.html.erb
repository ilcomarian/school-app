<%= form_with model:   @curent_student  do |form| %>
    <%= form.text_field :first_name, placeholder: "form.first_name"%>
    <%= form.text_field :last_name, placeholder: "form.last_name"%>
    <%= form.select :age, (18..149)%>
    <%= form.select :cohort_id,  Cohort.all.map {|x| [x.name,x.id ]} %>
    
    <%= form.submit %>
<% end %>

