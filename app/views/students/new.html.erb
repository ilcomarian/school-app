
<%= form_with model: Student.new do |form| %>
    <%= form.text_field :first_name, placeholder: "Enter first_name"%>
    <%= form.text_field :last_name, placeholder: "Enter last_name"%>
    <%= form.select :age, (4..17)%>
    <%= form.select :cohort_id, Cohort.all.map { |x|  [x.name, x.id] } %>
    <%= form.submit %>
<% end %>